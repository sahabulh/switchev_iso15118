[req]
prompt				= no
distinguished_name		= ca_dn

[ca_dn]
commonName			= USSAN123456789A
organizationName		= Sandia
organizationalUnitName		= EV
countryName			= US
domainComponent		= MO

[ext]
basicConstraints		= critical,CA:false
keyUsage			= critical,digitalSignature,nonRepudiation,keyEncipherment,keyAgreement
authorityKeyIdentifier		= critical,keyid,issuer
subjectKeyIdentifier		= critical,hash
